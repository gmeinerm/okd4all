[epel]
name=Extra Packages for Enterprise Linux $releasever - $basearch
baseurl=http://homepi.mark.lan/pxe/epel9/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-$releasever

[baseos]
name=Rocky Linux $releasever - BaseOS
baseurl=http://homepi.mark.lan/pxe/rockylinux9/BaseOS/x86_64/os/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9

[appstream]
name=Rocky Linux $releasever - AppStream
baseurl=http://homepi.mark.lan/pxe/rockylinux9/AppStream/x86_64/os/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9

[crb]
name=Rocky Linux $releasever - CRB
baseurl=http://homepi.mark.lan/pxe/rockylinux9/CRB/x86_64/os/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9
